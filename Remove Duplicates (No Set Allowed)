//Edge Cases: 1. Empty Array, 2. Array With Duplicates, 3.Array Contains Negative Values


//A. Brute Force - Time Complexity: O(N^2), Space Complexity: O(N) "Used an extra result array 'ans'." 

#include <bits/stdc++.h>
using namespace std;
int main() {
    vector<int>arr = {1,2,3,2,4,1,-1,-9};
    int n=arr.size();
    if(n<1) return -1;
    vector<int>ans;
    
    ans.push_back(arr[0]); //first number always be distinct
    for(int i=1;i<n;i++){
        bool check = true;
        for(int j=0;j<ans.size();j++){
            if(arr[i]==ans[j]){
                check = false;
            }
        }
            if(check){
                ans.push_back(arr[i]);
            }
    }
    int m = ans.size();
    for(int i=0;i<m;i++){
        cout<<ans[i]<<" ";
        
    }
    return 0;
}

//B. Optimized Way - Time Complexity: O(N), Space Complexity: O(N)

#include <bits/stdc++.h>
using namespace std;
int main() {
    vector<int>arr = {1,2,3,2,4,1,-1,-9};
    int n=arr.size();
    if(n<1) return -1;
    
    unordered_map<int,int>seen; //for checking the value occured before?
    vector<int>ans; //for storing result
    
    for(int i=0;i<n;i++){
        if(seen.count(arr[i])==0){
            ans.push_back(arr[i]);
            seen[arr[i]] = 1;
        }
    }
    int m = ans.size();
    for(int i=0;i<m;i++){
        cout<<ans[i]<<" ";
    }
    return 0;
}
